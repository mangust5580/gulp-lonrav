    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^2.0.3",
    "postcss": "^8.5.6",
    "sass": "^1.97.2",
    "sharp": "^0.34.5",
    "posthtml-expressions": "^1.11.4",
    "html-minifier-terser": "^7.2.0",
    "sitemap": "^8.0.0",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "png-to-ico": "^3.0.1"
